{
  "name": "@adatree-oss/atomic-components",
  "version": "4.5.2",
  "main": "dist/index.js",
  "repository": "https://gitlab.com/adatree-oss/atomic-design/atomic-components",
  "author": "Adatree",
  "license": "SEE LICENSE IN LICENSE FILE",
  "publishConfig": {
    "@adatree-oss:registry": "https://gitlab.com/api/v4/projects/32542974/packages/npm/"
  },
  "scripts": {
    "build": "rm -rf dist/ && rollup -c",
    "build:watch": "rollup -c -w",
    "format": "yarn prettier --write .",
    "lint": "eslint './src/**/*.{tsx,ts}' './storybook/**/*.{tsx,ts}'",
    "package": "yarn test && yarn lint && yarn build && npm publish",
    "storybook": "yarn --cwd ./storybook/ storybook",
    "generate-openapi-consent-client": "openapi-generator generate --skip-validate-spec -i https://raw.githubusercontent.com/Adatree/API-Specs/main/consent.yaml -g typescript-axios --additional-properties=modelPropertyNaming=original -o src/generated/consent",
    "generate-openapi-data-client": "openapi-generator generate --skip-validate-spec -i https://raw.githubusercontent.com/Adatree/API-Specs/main/data.yaml -g typescript-axios --additional-properties=modelPropertyNaming=original -o src/generated/data",
    "test": "jest"
  },
  "dependencies": {
    "@mui/lab": "^5.0.0-alpha.67",
    "@mui/material": "^5.2.7",
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest",
    "@mui/styled-engine-sc": "^5.1.0",
    "axios": "^0.25.0",
    "date-fns": "^2.28.0",
    "lodash": "^4.17.21",
    "mdi-material-ui": "7.2.0",
    "styled-components": "^5.3.3",
    "uuid": "^8.3.2"
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^1.0.10-4.2.3",
    "@types/lodash": "4.14.184",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.38",
    "@types/styled-components": "^5.1.19",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^27.4.7",
    "prettier": "2.5.1",
    "process": "^0.11.10",
    "rollup": "^2.63.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  }
}
