{
  "name": "@adatree/atomic-components",
  "version": "5.0.3",
  "main": "dist/index.js",
  "repository": "https://github.com/Adatree/atomic-components.git",
  "author": "Adatree",
  "license": "SEE LICENSE IN LICENSE FILE",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "type": "module",
  "scripts": {
    "build": "rm -rf dist/ && rollup -c",
    "build:watch": "rollup -c -w",
    "format": "yarn prettier --write .",
    "lint": "eslint './src/**/*.{tsx,ts}' './storybook/**/*.{tsx,ts}'",
    "package": "yarn test && yarn lint && yarn build && npm publish",
    "storybook": "yarn --cwd ./storybook/ storybook",
    "generate-openapi-consent-client": "openapi-generator-cli generate --skip-validate-spec -i https://raw.githubusercontent.com/Adatree/API-Specs/main/consent.yaml -g typescript-axios --additional-properties=modelPropertyNaming=original -o src/generated/consent",
    "generate-openapi-data-client": "openapi-generator-cli generate --skip-validate-spec -i https://raw.githubusercontent.com/Adatree/API-Specs/main/data.yaml -g typescript-axios --additional-properties=modelPropertyNaming=original -o src/generated/data",
    "test": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/lab": "^5.0.0-alpha.117",
    "@mui/material": "^5.11.6",
    "@mui/styled-engine": "latest",
    "@mui/x-date-pickers": "^5.0.16",
    "axios": "^0.25.0",
    "date-fns": "^2.28.0",
    "lodash": "^4.17.21",
    "mdi-material-ui": "7.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/jest": "^27.4.0",
    "@types/lodash": "4.14.184",
    "@types/react": "^17.0.38",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "jest": "^27.4.7",
    "postcss": "^8.4.20",
    "prettier": "2.5.1",
    "process": "^0.11.10",
    "rollup": "^3.11.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  }
}
